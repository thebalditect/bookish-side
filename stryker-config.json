{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.CLI/config.schema.json",
  "stryker-config": {
    "reporters": [
      "html",
      "json",
      "progress"
    ],
    "coverage-analysis": "perTestInIsolation",
    "thresholds": {
      "high": 85,
      "low": 80,
      "break": 75
    },
    "solution": "./MyBookishSide.sln",
    "configuration": "Release",
    "test-projects": [
      "src/**/*.UnitTests.csproj"
    ]
  }
}
